{
    "name": "astrotomic/ignition-stackoverflow",
    "description": "A tab that fetches stackoverflow questions",
    "keywords": [
        "laravel",
        "flare",
        "ignition"
    ],
    "homepage": "https://astrotomic.info",
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Witkowski",
            "email": "gummibeer@astrotomic.info",
            "homepage": "https://gummibeer.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "facade/ignition": "^1.0 || ^2.0",
        "facade/ignition-contracts": "^1.0",
        "illuminate/support": "5.8.* || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.8.* || ^4.0 || ^5.0 || ^6.0",
        "phpunit/phpunit": "^8.3 || ^9.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Astrotomic\\IgnitionStackOverflowTab\\TabServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Astrotomic\\IgnitionStackOverflowTab\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Astrotomic\\IgnitionStackOverflowTab\\Tests\\": "tests"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html=build"
    },
    "support": {
        "email": "dev@astrotomic.info",
        "issues": "https://github.com/Astrotomic/ignition-stackoverflow/issues",
        "source": "https://github.com/Astrotomic/ignition-stackoverflow"
    }
}
